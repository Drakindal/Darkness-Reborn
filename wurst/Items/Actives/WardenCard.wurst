package WardenCard

import ClosureEvents
import ClosureForGroups
import codeDamageTypings
integer u_i = 0

init
    wardenCard()

function wardenCard()
    EventListener.onCast('A0TE') caster ->
        u_i = 0
        forUnitsInRange(caster.getPos(), 400) (unit u) ->
            if(u.isEnemyOf(caster))
                if(u_i < 8)
                    caster.physicalDamageCode(25000, u, true)
                    u_i++