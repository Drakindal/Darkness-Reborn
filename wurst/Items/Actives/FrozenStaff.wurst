package FrozenStaff

import ClosureEvents

init
    frozenStaff()

function frozenStaff()
    EventListener.onTargetCast('A0YB') (unit caster, unit target) ->
        let dam = caster.getInt(true) * 45.00
        caster.damageTarget(target, dam, false, false, ATTACK_TYPE_CHAOS, DAMAGE_TYPE_UNIVERSAL, WEAPON_TYPE_WHOKNOWS)