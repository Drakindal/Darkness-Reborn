package FrostWand
import codeDamageTypings
import ClosureEvents

init
    frostWand()
    
function frostWand()
    EventListener.onTargetCast('A0YA') (unit caster, unit target) ->
        let dam = caster.getInt(true) * 15.00
        caster.damageTargetCode(dam, target, null, -1, damageType.MAGICAL)