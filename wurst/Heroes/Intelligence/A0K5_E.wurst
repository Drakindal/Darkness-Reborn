package A0K5_E
import ClosureEvents
import ClosureForGroups
import ClosureTimers
init
    timeStop()
function timeStop()
    EventListener.onCast('A0K5') (unit caster) ->
        forUnitsInRange(caster.getPos(), 99999, true) (unit u) ->
            if caster.isEnemyOf(u)
                u.pause()
                doAfter(10) -> 
                    u.unpause()