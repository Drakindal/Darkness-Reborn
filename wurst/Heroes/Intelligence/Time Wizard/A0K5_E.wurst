package A0K5_E
import ClosureEvents
import OrderIds
import MyAbilityIds
import DummyCaster
init
    timeStop()
function timeStop()
    EventListener.onCast('A0K5') (unit caster) ->
        new DummyCaster()
        ..origin(caster.getPos())
        ..owner(caster.getOwner())
        ..castImmediate(ABILITY_DUMMY_18, 1, OrderIds.stomp)
        ..delay(0)