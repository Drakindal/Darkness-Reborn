package wolfmanHowl

import ClosureEvents
import MyAbilityIds
import MyUnitIds
import ClosureTimers

function howl()
    EventListener.onCast(WOLF_INITIATE_ABIL_2) (unit caster) ->
        let u = createUnit(DUMMY_UNIT_0)
        u.addAbility(WOLF_FINAL_DUMMY_1, 1)
        doAfter(10) -> 
            u.remove()

init
    howl()