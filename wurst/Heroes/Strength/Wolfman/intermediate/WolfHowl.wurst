package WolfHowl

import ClosureEvents
import MyAbilityIds
import DummyCaster
import OrderIds

function howl()
    EventListener.onCast(WOLF_INITIATE_ABIL_2) (unit caster) ->
        new DummyCaster()
        ..origin(caster.getPos())
        ..owner(caster.getOwner())
        ..castImmediate(WOLF_INITIATE_DUMMY_1, 
        caster.getAbilityLevel(WOLF_INITIATE_ABIL_2), 
        OrderIds.stomp)

init
    howl()