package WolfHowl

import ClosureEvents
import MyAbilityIds
import MyUnitIds
import ClosureTimers

function howl()
    EventListener.onCast(WOLF_INITIATE_ABIL_2) (unit caster) ->
        let u = createUnit(DUMMY_UNIT_0)
        u.addAbility(WOLF_INITIATE_DUMMY_1, caster.getAbilityLevel(WOLF_INITIATE_ABIL_2))
        doAfter(10) -> 
            u.remove()

init
    howl()