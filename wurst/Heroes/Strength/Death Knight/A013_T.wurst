package A013_T
import ClosureEvents
import ClosureTimers
import A00L_Q
import MyAbilityIds

public function soulsKing()
    EventListener.onCast(ABILITY_8) (unit caster) ->
        let boost = souls * 10
        caster.addStr(boost)
        doAfter(caster.getAbility(ABILITY_8).getField(ABILITY_RLF_DURATION_HERO, caster.getAbilityLevel(ABILITY_8))) -> 
            caster.setStr(caster.getStr(false)-boost)