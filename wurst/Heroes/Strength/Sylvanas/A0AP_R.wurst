package A0AP_R
import ClosureEvents

public function longRange()
    EventListener.add(EVENT_PLAYER_HERO_SKILL) ->
        let abi = GetLearnedSkill()
        if abi == 'A0AP'
            let u = GetTriggerUnit()
            let lvl = u.getAbilityLevel(abi)
            let p = u.getOwner()
            SetPlayerTechResearched(p, 'R001', lvl)